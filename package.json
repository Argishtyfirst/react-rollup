{
  "name": "@setup/neon-branding",
  "version": "0.0.1",
  "main": "dist/es/prod.js",
  "description": "branding page for workfront new experience",
  "scripts": {
    "prepublishOnly": "npm run release",
    "release": "npm run build:dev && npm run build:prod",
    "build:dev": "NODE_ENV=development rollup -c",
    "build:prod": "NODE_ENV=production rollup -c",
    "build": "NODE_ENV=production rollup -c",
    "storybook": "start-storybook -p 9609",
    "build-storybook": "build-storybook",
    "test": "jest src",
    "watch": "NODE_ENV=production rollup -c ./rollup.config.js --watch",
    "pretty-quick": "pretty-quick",
    "ci-build": "npm run build",
    "lint": "eslint --ext js,jsx src",
    "lint:fix": "eslint --ext js,jsx src --fix",
    "format": "prettier --write \"src/**/*.jsx\""
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.workfront.tech/solaris/neon-branding"
  },
  "keywords": [
    "rollup",
    "styled-jsx",
    "components",
    "library",
    "package"
  ],
  "author": "Argishti Stepanyan",
  "license": "ISC",
  "homepage": "https://gitlab.workfront.tech/solaris/neon-branding",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@commitlint/cli": "^8.3.5",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-config-prettier": "^6.11.0",
    "babel-eslint": "^10.1.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.7.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-terser": "^5.3.0",
    "@storybook/react": "^5.3.18",
    "@testing-library/jest-dom": "^5.5.0"
  },
  "peerDependencies": {
    "react": ">= 16.x.x",
    "react-dom": ">= 16.x.x"
  }
}
